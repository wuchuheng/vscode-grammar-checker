{
  "name": "grammar-checker",
  "displayName": "grammar checker",
  "description": "A grammar checker incorect the pure text file in standard english grammar.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript",
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:markdown"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "GrammarChecker.check",
        "title": "Grammar checker: check"
      },
      {
        "command": "GrammarChecker.fix",
        "title": "Grammar checker: fix"
      },
      {
        "command": "GrammarChecker.removeApiKey",
        "title": "Grammar checker: remove api key"
      }
    ],
    "diagnostics": {
      "languages": [
        "typescript",
        "javascript",
        "javascriptreact",
        "typescriptreact",
        "json",
        "jsonc",
        "markdown"
      ],
      "documentSelector": [
        "typescript",
        "javascript",
        "javascriptreact",
        "typescriptreact",
        "json",
        "jsonc",
        "markdown"
      ]
    },
    "configuration": {
      "type": "object",
      "title": "Grammar checker Extension Configuration",
      "properties": {
        "grammarChecker.apiKey": {
          "default": "",
          "description": "The secret key of the model will be used to access the model."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "pnpm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "rm -rf out; pnpm run compile-tests && pnpm run compile && pnpm run lint",
    "lint": "eslint src --ext ts",
    "test": "node out/test/mainTest.js",
    "test:tmp": "mocha -r ts-node/register tmp/tmp.test.ts --timeout 20000"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "~20.14.2",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "glob": "^10.3.10",
    "mocha": "^10.6.0",
    "prettier": "3.3.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vscode-test": "^1.6.1",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@wuchuhengtools/helper": "^1.0.18",
    "axios": "^1.7.2",
    "dotenv": "^16.4.5",
    "openai": "^4.51.0",
    "vscode-languageclient": "^9.0.1",
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.11"
  }
}